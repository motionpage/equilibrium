			"settings": {
				"foreground": "#FF79C6"
			}
		},
		{
			"name": "HTML tags",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#FF79C6"
			}
		},
		{
			"name": "CSS attribute parent selectors ('&')",
